<%- include( "partials/header"); -%>

<div class="w">
  <h1>Home</h1>

  <p><%= homeStartingContent %></p>

  <% posts.forEach(function(post){ %>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content %>

    <a href="/posts/<%= post._id %>">Read More</a>
  </p>
  <input type="hidden" name="postId" value="<%= post._id %>" />
  <% }) %>
</div>

<%- include( "partials/footer"); -%>
